{
  "usesTypeScript": true,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "path": "src/components",
      "customTemplates": {
        "component": "src/templates/component/TemplateName.tsx"
      },
      "withLazy": false,
      "withStory": false,
      "withStyle": false,
      "withTest": false
    },
    "block": {
      "path": "src/blocks",
      "customTemplates": {
        "component": "src/templates/block/TemplateName.tsx"
      },
      "withLazy": false,
      "withStory": false,
      "withStyle": false,
      "withTest": false
    }
  },
  "usesStyledComponents": false
}
